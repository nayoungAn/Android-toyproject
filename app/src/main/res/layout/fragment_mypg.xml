<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".ResultActivity" android:background="#FFFFFF">


	<Button
			android:text="로그아웃"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/btnLogout"
			android:backgroundTint="#306839"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
			android:layout_marginTop="320dp" app:layout_constraintHorizontal_bias="0.773"/>
	<Button
			android:text="등록하기"
			android:layout_width="104dp"
			android:layout_height="48dp" android:id="@+id/GalleryButton"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/imageView2"
			android:backgroundTint="#306839"
			android:layout_marginTop="20dp"/>
	<ImageView
			android:background="@drawable/img"
			android:layout_width="150dp"
			android:layout_height="150dp"
			tools:src="@drawable/img"
			android:id="@+id/imageView2"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
			android:layout_marginTop="96dp" app:layout_constraintHorizontal_bias="0.498"/>
	<TextView
			android:text="currentUser"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/currentUser"
			android:textSize="20dp"
			android:textStyle="bold"
			app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="@+id/linearLayout"
			app:layout_constraintEnd_toStartOf="@+id/btnLogout" app:layout_constraintTop_toTopOf="@+id/linearLayout"
			android:layout_marginTop="150dp"/>
	<LinearLayout
			android:orientation="vertical"
			android:layout_width="match_parent"
			android:layout_height="match_parent" tools:layout_editor_absoluteY="35dp"
			tools:layout_editor_absoluteX="0dp" android:id="@+id/linearLayout">
		<TextView
				android:text="My Page"
				android:layout_width="match_parent"
				android:layout_height="50dp" android:id="@+id/textView4"
				android:textColor="#FFFFFF" android:textAlignment="center" android:textSize="30sp"
				android:background="#306839" android:textStyle="bold|italic"/>
	</LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>